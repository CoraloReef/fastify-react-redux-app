{
   "name": "fastify-react-redux-app",
   "version": "0.1.0",
   "description": "Boilertplate Node.js application with simple authorization",
   "keywords": [
      "fastify",
      "sqlite",
      "nodejs",
      "react",
      "redux",
      "toolkit",
      "scss",
      "i18next",
      "pug",
      "eslint"
   ],
   "main": "",
   "scripts": {
      "test": "jest",
      "build-configs": "babel ormconfig.js webpack.config.js --out-dir dist",
      "build-server": "NODE_ENV=production babel server --out-dir dist/server --source-maps inline && npm run build-configs && cp -r server/views dist/server/views",
      "build-frontend": "NODE_ENV=production npx webpack -p",
      "build": "rm -rf dist && npm run build-frontend && npm run build-server",
      "start": "node dist/server/bin/server.js",
      "develop": "npx nodemon --exec DEBUG=application:* npx babel-node server/bin/server.js"
   },
   "author": "Viacheslav Gordeiko",
   "license": "ISC",
